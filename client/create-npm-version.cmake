cmake_minimum_required(VERSION 2.8)
file(WRITE ${OUTPUT} "")
execute_process(COMMAND jq .version ${INPUT} OUTPUT_VARIABLE VERSION ERROR_VARIABLE ERROR RESULT_VARIABLE RESULT OUTPUT_STRIP_TRAILING_WHITESPACE)
message(STATUS "NPM Version ${VERSION}")
file(APPEND ${OUTPUT} "const char *${VARIABLE} = ${VERSION};\n")
